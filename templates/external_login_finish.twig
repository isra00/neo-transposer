{% extends "base.twig" %}

{% block header %}{% endblock %}

{% block page_class %}external-login-return{% endblock %}

{% block content %}

<div class="spinner"></div>

<p>{% trans %}Login succesful - returning to the app.{% endtrans %}</p>

<script>
function setUserToken(token)
{
